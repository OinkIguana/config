"*":
  "arduino-upload": {}
  "atom-package-deps":
    ignored: []
  "autoclose-html": {}
  "autocomplete-racer":
    hyperclick: {}
  "code-peek":
    ignoredPaths: [
      "node_modules"
    ]
  core:
    closeEmptyWindows: false
    disabledPackages: [
      "markdown-preview"
    ]
    ignoredNames: [
      ".git"
      "._*"
      ".class"
      ".o"
      ".d"
    ]
    packagesWithKeymapsDisabled: [
      "goto-definition"
    ]
    projectHome: "/home/cam/code/"
    telemetryConsent: "limited"
  editor:
    fontFamily: "Hack"
    invisibles: {}
    nonWordCharacters: "/\\()\"':,.;<>~!@#$%^&*|+=[]{}`?-…。，"
    preferredLineLength: 100
    showInvisibles: true
    tabLength: 2
    tabType: "soft"
  "exception-reporting":
    userId: "3c06fa62-dc73-c538-c016-d87640d0ebd6"
  "go-plus":
    config: {}
  "goto-definition": {}
  hex:
    bytesPerLine: 4
  hyperclick:
    linuxTriggerKeys: "metaKey"
  "ide-haskell":
    autoHideOutput: true
  "ide-rust":
    rlsToolchain: "stable"
  "language-babel": {}
  "language-elm":
    elmOraclePath: "/home/cam/code/nvm/versions/node/v8.6.0/bin/elm-oracle"
  latex:
    opener: "pdf-view"
  "latex-completions": {}
  linter:
    lintOnChange: false
    lintOnChangeInterval: 0
    lintPreviewTabs: false
  "linter-gcc":
    execPath: "/home/cam/sys161/bin/cs350-gcc"
    gccDefaultCppFlags: "-c -Wall -std=c++14 -fsyntax-only"
    gccErrorLimit: -1
  "linter-markdown":
    detectIgnore: false
  "linter-scalac":
    compileAllClassesOnLint: true
    compileClassesToClasspath: true
  "linter-ui-default":
    panelHeight: 110
  "markdown-preview-plus":
    enableLatexRenderingByDefault: true
    useLazyHeaders: false
  minimap: {}
  platomformio:
    verboseUpload: false
  racer:
    racerBinPath: "/home/cam/.cargo/bin/racer"
    rustSrcPath: "/home/cam/.rustup/toolchains/stable-x86_64-unknown-linux-gnu/lib/rustlib/src/rust/src"
  "run-in-terminal":
    launchers: "webpack.config.js webpack, package.json npm start"
    terminal: "/usr/bin/gnome-terminal"
    terminal_exec_arg: "-e"
    use_exec_working_directory: false
  script:
    cwdBehavior: "Directory of the script"
    stopOnRerun: true
  "spell-check":
    grammars: [
      "source.asciidoc"
      "source.gfm"
      "text.git-commit"
      "text.plain"
      "text.plain.null-grammar"
      "text.tex.latex"
    ]
  "terminal-plus":
    core: {}
    toggles:
      autoClose: true
  "tree-view":
    hideVcsIgnoredFiles: true
  welcome:
    showOnStartup: false
  wordcount:
    alwaysOn: true
".c.source":
  editor:
    tabLength: 4
".kotlin.source":
  editor:
    tabLength: 4
".source.swift":
  editor:
    tabLength: 4
